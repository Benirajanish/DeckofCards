


@{
    ViewBag.Title = "Deck";
}
<h2>Deck</h2>
<table class="table-striped">
    <thead>
        <tr>
            <th>
                <strong>Deck Shuffled!</strong>
            </th>
        </tr>
    </thead>

    @for (int i = 0; i < ViewBag.Cards.Count; i++)
    {
        <tr class="col-md-4" align="left">

            <td>

                
         
               <strong>@ViewBag.Cards[i].value of @ViewBag.Cards[i].suit</strong>
                <br />
                <img src="@ViewBag.Cards[i].image" />

              
            </td>
        </tr>
    }

</table>